{
  "name": "casio",
  "displayName": "Casio Theme",
  "description": "A theme for people what doesn't conform with nord and want to get a better theme idk",
  "version": "1.6.0",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Themes"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint ./themes/** --ext .json,.js --fix",
    "format": "prettier --write ./themes/**"
  },
  "contributes": {
    "themes": [
      {
        "label": "Casio Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Casio-color-theme.json"
      },
      {
        "label": "Casio Theme Darker",
        "uiTheme": "vs-dark",
        "path": "./themes/Casio-Darker-theme.json"
      },
      {
        "label": "Casio Theme Darker Flat",
        "uiTheme": "vs-dark",
        "path": "./themes/Casio-Darker-Flat-theme.json"
      },
      {
        "label": "Casio Theme Flat",
        "uiTheme": "vs-dark",
        "path": "./themes/Casio-color-flat-theme.json"
      },
      {
        "label": "Casio Theme Dark Pro",
        "uiTheme": "vs-dark",
        "path": "./themes/Casio-color-Dark-Pro-theme.json"
      },
      {
        "label": "Casio Theme High Contrast",
        "uiTheme": "vs-dark",
        "path": "./themes/Casio-High-Contrast-theme.json"
      }
    ]
  },
  "repository": {
    "url": "https://github.com/ChirujanoCodding/Casio-theme"
  },
  "publisher": "ElShyrux",
  "icon": "public/logo.ico",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.7",
    "commitlint": "^17.6.7",
    "eslint": "^8.45.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.0"
  }
}
